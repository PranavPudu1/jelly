{
    "name": "jelly-server",
    "version": "1.0.0",
    "description": "Backend API for Jelly - Tinder-like restaurant discovery app",
    "main": "dist/index.js",
    "scripts": {
        "dev": "nodemon --exec ts-node src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "jest",
        "seed": "ts-node src/scripts/seedDatabase.ts"
    },
    "keywords": [
        "express",
        "typescript",
        "firebase",
        "firestore",
        "restaurant",
        "api"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.4.1",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "firebase-admin": "^12.0.0",
        "helmet": "^7.1.0"
    },
    "devDependencies": {
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.11.5",
        "@typescript-eslint/eslint-plugin": "^6.19.1",
        "@typescript-eslint/parser": "^6.19.1",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "nodemon": "^3.0.3",
        "prettier": "^3.2.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    }
}
